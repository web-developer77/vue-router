{
  "name": "vue-router",
  "version": "4.0.0-alpha.0",
  "main": "dist/vue-router.cjs.js",
  "browser": "dist/vue-router.esm.js",
  "unpkg": "dist/vue-router.js",
  "module": "dist/vue-router.esm-bundler.js",
  "types": "dist/src/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist",
    "playground",
    "README.md"
  ],
  "scripts": {
    "build": "yarn rollup -c rollup.config.js",
    "dev": "webpack-dev-server --mode=development",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build:playground": "webpack --env.prod",
    "dev:examples": "webpack-dev-server --mode=development --config examples/webpack.config.js",
    "lint": "prettier -c --parser typescript \"{src,__tests__,e2e}/**/*.[jt]s?(x)\"",
    "lint:fix": "yarn run lint --write",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "jest --coverage",
    "test": "yarn run test:types && yarn run test:unit && yarn build",
    "prepublishOnly": "yarn run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0-alpha.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/jest": "^25.1.3",
    "@types/jsdom": "^16.1.0",
    "@types/webpack": "^4.41.6",
    "@types/webpack-env": "^1.15.0",
    "@vue/compiler-sfc": "3.0.0-alpha.4",
    "axios": "^0.19.2",
    "browserstack-local": "^1.4.5",
    "chromedriver": "^80.0.1",
    "codecov": "^3.6.5",
    "conventional-changelog-cli": "^2.0.31",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "faked-promise": "^2.2.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-mock-warn": "^1.0.1",
    "nightwatch": "^1.3.4",
    "nightwatch-helpers": "^1.2.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "selenium-server": "^3.141.59",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "vue": "3.0.0-alpha.4",
    "vue-loader": "next",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
